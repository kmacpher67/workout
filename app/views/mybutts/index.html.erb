<h1>Listing mybutts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cturl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mybutts.each do |mybutt| %>
      <tr>
        <td><%= mybutt.name %></td>
        <td><%= mybutt.cturl %></td>
        <td><%= link_to 'Show', mybutt %></td>
        <td><%= link_to 'Edit', edit_mybutt_path(mybutt) %></td>
        <td><%= link_to 'Destroy', mybutt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mybutt', new_mybutt_path %>
